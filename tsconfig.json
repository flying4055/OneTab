{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "static/js/contracts/**/*.js",
    "static/js/services/navCommands.js",
    "static/js/services/perfBaseline.js",
    "static/js/services/searchIndex.js",
    "tests/**/*.mjs"
  ]
}
